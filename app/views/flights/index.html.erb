<h1>All Flights</h1>
<% @flights.each do |flight| %>
<section id= "flight-<%=flight.id %>">
<ul>
  <li>Flight Number: <%= flight.number %> Airline:<%=flight.airline.name  %></li>
</ul>
<% flight.passengers.each do |passenger| %>
<ul>
  <li>Passenger: <%= passenger.name  %> </li>
</ul>
<%end %>
<%end %>
</section>
